      <DESCENDANTS>
        <NODEREF id="faceCaixa1" />
        <NODEREF id="faceCaixa2" />
        <NODEREF id="faceCaixa3" />
        <NODEREF id="faceCaixa4" />
        <NODEREF id="faceCaixa5" />
